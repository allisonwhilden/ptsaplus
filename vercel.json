{
  "version": 2,
  "framework": "nextjs",
  "rootDirectory": "app",
  "installCommand": "cd .. && pnpm install --frozen-lockfile",
  "buildCommand": "cd .. && pnpm run build",
  "devCommand": "pnpm dev",
  "outputDirectory": ".next",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD .",
  "github": {
    "enabled": true,
    "autoAlias": true
  },
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 10
    }
  },
  "regions": ["iad1"],
  "crons": []
}
