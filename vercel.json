{
  "version": 2,
  "framework": "nextjs",
  "installCommand": "cd app && npx pnpm@8.15.9 install",
  "buildCommand": "cd app && npx pnpm@8.15.9 build",
  "devCommand": "cd app && pnpm dev",
  "outputDirectory": "app/.next",
  "functions": {
    "app/src/app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "regions": ["iad1"],
  "github": {
    "autoDeployment": true
  }
}